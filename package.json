{
  "name": "wibbo-discord",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "Wibbo Discord Bot",
  "main": "./src/main.ts",
  "dependencies": {
    "dayjs": "^1.11.10",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "net": "^1.0.2",
    "@wibbo/prisma": "../WibboPrisma"
  },
  "devDependencies": {
    "@types/node": "^20.11.1",
    "esbuild": "^0.20.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2"
  },
  "scripts": {
    "dev": "ts-node-dev src/main.ts",
    "build": "pnpm clean && pnpm esbuild && pnpm build-nodemodules",
    "start": "node ./dist/main.mjs",
    "clean": "rm -rf ./dist",
    "esbuild": "esbuild src/main.ts --bundle --platform=node --packages=external --outfile=dist/main.mjs --format=esm",
    "build-nodemodules": "cp -r package.json ./dist && cd ./dist && npm install --production"
  },
  "author": "JasonDhose",
  "license": "ISC"
}
