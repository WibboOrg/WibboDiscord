{
  "name": "wibbo-discord",
  "version": "1.0.0",
  "description": "Wibbo Discord Bot",
  "main": "./src/Main.ts",
  "dependencies": {
    "cli-color": "^2.0.0",
    "dayjs": "^1.10.7",
    "discord.js": "^14.4.0",
    "mysql": "^2.18.1",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@types/cli-color": "^2.0.1",
    "@types/mysql": "^2.15.17",
    "@types/node": "^18.6.2",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "devjs": "concurrently -k -n \"Typescript,Node\" -p \"[{name}]\" -c \"blue,green\" \"tsc --watch\" \"nodemon dist/Main.js\"",
    "dev": "ts-node-dev --respawn --transpile-only src/Main.ts",
    "start": "node ./dist/Main.js",
    "build": "yarn clean & tsc",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix"
  },
  "author": "",
  "license": "ISC"
}
